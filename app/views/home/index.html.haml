.wrapper
  = render "shared/main_nav_bar"

  .main-panel
    = render "shared/top_nav_bar"
    - iframe_url = "http://localhost:3000"
    .content
      .container-fluid
        .row
          .col-md-4
            .card
              %iframe{ src: "#{iframe_url}/list_views", style: "width: 250px; height: 300px; border: 0;" }

              .card-footer.bg-light
                .legend
                  List view embed code:
                  %br
                  .text-primary= "<iframe src=\"#{iframe_url}/list_views\" style=\"width: 250px; height: 300px; border: 0;\"></iframe>"

          .col-md-4
            .card
              %iframe{ src: "#{iframe_url}/list_views?open_tab=00", style: "width: 250px; height: 300px; border: 0;" }

              .card-footer.bg-light
                .legend
                  List view with open tab embed code:
                  %br
                  .text-primary= '<iframe src="#{iframe_url}/list_views?open_tab=00" style="width: 250px; height: 300px; border: 0;"></iframe>'


          .col-md-4
            .card{ style: "width: 350px;" }
              %iframe{ src: "#{iframe_url}/?embed_type=mobile", style: "height: 568px; width: 320px; border: 0;" }

              .card-footer.bg-light
                .legend
                  Mobile view embed code:
                  %br
                  .text-primary= '<iframe src="#{iframe_url}/?embed_type=mobile" style="width: 320px; height: 500px; border: 0;"></iframe>'
        .row
          .col-md-8
            .card
              %iframe{ src: "#{iframe_url}/?embed_type=map", style: "height: 500px; width: 530; border: 0;" }
              .card-footer.bg-light
                .legend
                  With info tab closed embed code:
                  %br
                  .text-primary= '<iframe src="#{iframe_url}/?embed_type=map" style="height: 500px; width: 530; border: 0;"></iframe>'

        .row
          .col-md-12
            .card
              %iframe{ src: "#{iframe_url}", style: "height: 600px;  border: 0;" }
              .card-footer.bg-light
                .legend
                  Desktop view embed code:
                  %br
                  .text-primary= '<iframe src="#{iframe_url}/" style="height: 600px;  border: 0;"></iframe>'
